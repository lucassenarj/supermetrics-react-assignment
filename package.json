{
  "name": "supermetrics-react-assignment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "node-sass": "^6.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "semantic-release --no-ci"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "axios-mock-adapter": "^1.19.0",
    "prettier": "^2.3.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.4",
    "semantic-release": "^17.4.4"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.spec.js",
      "src/**/index.js",
      "!src/**/types.js",
      "!src/**/tests/",
      "!src/**/__snapshots__/",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/setupTests.js",
      "src/store/index.js",
      "src/store/actions/index.js",
      "src/utils/mocks/articles.js",
      "src/utils/mocks/login.js",
      "src/utils/mocks/posts.js",
      "src/utils/mocks/users.js",
      "src/pages/About/index.js",
      "src/pages/Contact/index.js",
      "src/pages/Dashboard/index.js",
      "src/pages/Login/index.js"
    ]
  }
}
